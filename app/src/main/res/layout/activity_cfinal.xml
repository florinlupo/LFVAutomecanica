<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/aaaaa"
    tools:context=".Cfinal">

    <Button
        android:id="@+id/paginap"
        android:layout_width="176dp"
        android:layout_height="61dp"
        android:layout_marginTop="384dp"
        android:text="Meniu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/reiab"
        android:layout_width="176dp"
        android:layout_height="61dp"
        android:layout_marginTop="44dp"
        android:text="Incepe de la inceput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/paginap" />


    <Button
        android:id="@+id/reiadela3"
        android:layout_width="176dp"
        android:layout_height="61dp"
        android:layout_marginTop="44dp"
        android:text="Reia parte de caroserie"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reiab" />

    <TextView
        android:id="@+id/textView44"
        android:layout_width="198dp"
        android:layout_height="92dp"
        android:gravity="center_horizontal"
        android:text="Felictiari! Ai ajuns la final"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.074" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="327dp"
        android:layout_height="194dp"
        android:gravity="center_horizontal"
        android:text="Ai terminat cu succes tot programu dostru de invatare, daca nu esti sigur pe tine poti oricand sa refaci toti pasii,dar daca ai inteles tot inseamna ca informatiile au fost bine prezentate si poti recomanda si altora"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.251" />
</androidx.constraintlayout.widget.ConstraintLayout>